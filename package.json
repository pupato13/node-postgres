{
    "name": "postgres",
    "version": "0.0.1",
    "description": "",
    "scripts": {
        "start": "node -r dotenv/config dist/main",
        "dev": "ts-node-dev --respawn --transpile-only -r dotenv/config --inspect=0.0.0.0:9229 src/main",
        "clean": "rimraf dist",
        "build": "npm run clean && tsc -p tsconfig-build.json",
        "prepare": "husky install",
        "lint": "eslint .",
        "lint:fix": "npm run lint -- --fix",
        "test": "jest --passWithNoTests --no-cache --runInBand",
        "test:watch": "npm test -- --watch",
        "test:staged": "npm test -- --findRelatedTests",
        "test:coverage": "npm test -- --coverage",
        "test:fb-api": "npm test -- -c ./jest.integration.config.js tests/external/facebook-api.test.ts",
        "test:s3": "npm test -- -c ./jest.integration.config.js tests/external/aws-s3-file-storage.test.ts"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/pupato13/node-postgres.git"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/pupato13/node-postgres/issues"
    },
    "homepage": "https://github.com/pupato13/node-postgres#readme",
    "devDependencies": {
        "@jest-mock/express": "^1.4.5",
        "@types/aws-sdk": "^2.7.0",
        "@types/axios": "^0.14.0",
        "@types/cors": "^2.8.12",
        "@types/express": "^4.17.13",
        "@types/jest": "^27.0.2",
        "@types/jsonwebtoken": "^8.5.6",
        "@types/module-alias": "^2.0.1",
        "@types/node": "^16.10.1",
        "@types/supertest": "^2.0.11",
        "@types/uuid": "^8.3.3",
        "@typescript-eslint/eslint-plugin": "^4.0.1",
        "dotenv": "^10.0.0",
        "eslint": "^7.12.1",
        "eslint-config-standard-with-typescript": "^21.0.1",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^5.0.0",
        "husky": "^7.0.4",
        "jest": "^27.3.1",
        "jest-mock-extended": "^2.0.4",
        "lint-staged": "^12.0.2",
        "pg-mem": "^2.1.9",
        "supertest": "^6.1.6",
        "ts-jest": "^27.0.7",
        "ts-node-dev": "^1.1.8",
        "typescript": "^4.4.4"
    },
    "engines": {
        "node": "16.x"
    },
    "dependencies": {
        "aws-sdk": "^2.1048.0",
        "axios": "^0.24.0",
        "cors": "^2.8.5",
        "express": "^4.17.1",
        "jsonwebtoken": "^8.5.1",
        "module-alias": "^2.2.2",
        "pg": "^8.7.1",
        "reflect-metadata": "^0.1.13",
        "rimraf": "^3.0.2",
        "typeorm": "^0.2.29",
        "uuid": "^8.3.2"
    }
}
